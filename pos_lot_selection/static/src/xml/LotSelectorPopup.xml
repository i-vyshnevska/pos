<templates id="template" xml:space="preserve">

        <!-- <t t-inherit="pos_event_sale.EventRegistrationReceipt" t-inherit-mode="extension"> -->
   

        <t t-name="LotSelectorPopup" owl="1" >
            <div role="dialog" class="modal-dialog">
                <div class="popup">
                    <header class="title ">
                        <t t-esc="props.title"/>
                    </header>
                    <main class="body">
                        <select id="lot_select" class="popup-input">
                            <option value="">Select Lot ID</option>
                            <t t-debug="pdb"></t>
                            <t t-foreach="lots" t-as="lot" >
                                <!-- <t t-set="display_name" t-value="props.lot_data[lot].name"/> -->
                                <span> some</span>
                            </t>
                        </select>
                    </main>
                    <footer class="footer">
                        <div class="button confirm" t-on-click="confirm">
                            Ok
                        </div>
                        <div class="button cancel" t-on-click="cancel">
                            Cancel
                        </div>
                    </footer>
                </div>
            </div>
        </t>

</templates>
